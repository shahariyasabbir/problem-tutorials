#light oj tomi mia problem
#include <bits/stdc++.h>
using namespace std;

long long factorial(int n) {
    long long result = 1;
    for (int i = 2; i <= n; i++) {
        result *= i;
    }
    return result;
}

void solve() {
    int t;
    cin >> t;
    cin.ignore(); // Consume newline after T

    while (t--) {
        string sentence;
        getline(cin, sentence);

        // Count the number of words in the sentence
        stringstream ss(sentence);
        int wordCount = 0;
        string word;
        while (ss >> word) {
            wordCount++;
        }

        // Compute factorial of wordCount
        long long fact = factorial(wordCount);

        // Output the probability as 1/fact
        cout << "1/" << fact << "\n";
    }
}

int main() {
    solve();
    return 0;
}
